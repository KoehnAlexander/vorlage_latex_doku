@article{Schryen.2002,
 abstract = {Electronic paper is a new material-technological stream in display research: electronic paper consists of a thin, flexible plastic film within which charged particles are aligned due to electric fields. Advantages over traditional technologies like LCD displays include lower production costs (lower process costs as well as lower material costs) and lower weight. Furthermore a better readability emerges because of better reflection, contrast, and larger viewing angles. This potential can be tapped for display panels, computer displays, and mobile devices. Almost every material can be used as substrate, hence flexible ones can be used as media for newspapers and journals. The leading US companies E Ink and Gyricon Media offer an increasingly broader range of products based on electronic paper.},
 author = {Schryen, Guido and Karla, J{\"u}rgen},
 year = {2002},
 title = {Elektronisches Papier --- Displaytechnologie mit weitem Anwendungsspektrum},
